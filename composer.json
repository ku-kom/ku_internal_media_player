{
    "name": "university-of-copenhagen/ku-internal-media-player",
    "type": "typo3-cms-extension",
    "description": "Plays (and loops) internal media files such as .mp4, .ogg, etc. including only play/pause button.",
    "homepage": "https://www.ku.dk/",
    "license": ["GPL-2.0-or-later"],
    "keywords": ["TYPO3 CMS"],
    "require": {
        "bk2k/bootstrap-package": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "UniversityOfCopenhagen\\KuInternalMediaPlayer\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "ku_internal_media_player"
        }
    }
}
